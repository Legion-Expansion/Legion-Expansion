{
  "parserOptions": {
    "ecmaFeatures": {
      "impliedStrict": true
    }
  },
  "env": {
    "browser": true,
    "jquery": true,
    "amd": true
  },
  "extends": ["eslint:recommended", "plugin:lodash/v3", "prettier"],
  "globals": {
    "api": "readonly",
    "model": "readonly",
    "_": "readonly",
    "loc": "readonly",
    "locTree": "readonly",
    "ko": "readonly",
    "engine": "readonly",
    "loadCSS": "readonly",
    "loadHtml": "readonly",
    "loadScript": "readonly",
    "Build": "readonly",
    "handlers": "readonly"
  },
  "plugins": ["lodash"],
  "rules": {
    "no-inner-declarations": "off",
    "lodash/prefer-lodash-method": 0
  }
}
